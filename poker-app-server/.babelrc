{
    "presets": [
        "@babel/preset-env",
        "@babel/typescript"
    ],
    "plugins": [
        ["babel-plugin-transform-builtin-extend", {
            "globals": ["Error", "Array"]
        }],
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "@babel/plugin-syntax-dynamic-import"
    ]
}
